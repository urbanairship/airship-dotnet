<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <NoBindingEmbedding>true</NoBindingEmbedding>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ProjectGuid>{A2B3C4D5-E6F7-8901-2345-6789ABCDEF01}</ProjectGuid>
    <OutputType>Library</OutputType>
    <RootNamespace>UrbanAirship</RootNamespace>
    <AssemblyName>Airship.Net.iOS.ObjectiveC</AssemblyName>
    <AssemblyTitle>Airship iOS SDK - ObjectiveC</AssemblyTitle>
    <Description>Objective-C bindings for Airship SDK</Description>
    <TargetFramework>net8.0-ios</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>true</ImplicitUsings>
    <IsBindingProject>true</IsBindingProject>
    <IsTrimmable>false</IsTrimmable>
    <AssemblyVersion>$(AirshipIosVersion)</AssemblyVersion>
    <PackageVersion>$(AirshipIosNugetVersion)</PackageVersion>
    <IPhoneResourcePrefix>Resources</IPhoneResourcePrefix>
    <SupportedOSPlatformVersion>15.0</SupportedOSPlatformVersion>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug</OutputPath>
    <DefineConstants>DEBUG;</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>full</DebugType>
    <Optimize>true</Optimize>
    <OutputPath>bin\Release</OutputPath>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\UACrossPlatformVersion.cs">
      <Link>Properties\UACrossPlatformVersion.cs</Link>
    </Compile>
    <Compile Include="..\SharedAssemblyInfo.Common.cs">
      <Link>Properties\SharedAssemblyInfo.Common.cs</Link>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <ObjcBindingApiDefinition Include="ApiDefinitions.cs" />
  </ItemGroup>
  <ItemGroup>
    <ObjcBindingCoreSource Include="StructsAndEnums.cs" />
  </ItemGroup>

  <ItemGroup>
    <NativeReference Include="..\..\frameworks\AirshipObjectiveC.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
    </NativeReference>
    <!-- AirshipObjectiveC dependencies -->
    <NativeReference Include="..\..\frameworks\AirshipCore.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
    </NativeReference>
    <NativeReference Include="..\..\frameworks\AirshipBasement.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
    </NativeReference>
    <NativeReference Include="..\..\frameworks\AirshipAutomation.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
    </NativeReference>
    <NativeReference Include="..\..\frameworks\AirshipMessageCenter.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
    </NativeReference>
    <NativeReference Include="..\..\frameworks\AirshipPreferenceCenter.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
    </NativeReference>
    <NativeReference Include="..\..\frameworks\AirshipFeatureFlags.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
    </NativeReference>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Additions\" />
  </ItemGroup>

</Project>